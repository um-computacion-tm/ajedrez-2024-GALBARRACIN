[run]
source = main
omit =
    */__init__.py
    */tests/*
    venv/*
    .circleci/*
    .git/*
    */setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    def setUp
    def tearDown
    def suite
    def test_
    if __name__ == "__main__"
    print
    return raw_input

# Considerar cualquier archivo que esté vacío como completamente cubierto
[report]
ignore_errors = True
